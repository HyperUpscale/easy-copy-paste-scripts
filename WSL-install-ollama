wget https://developer.download.nvidia.com/compute/cuda/repos/wsl-ubuntu/x86_64/cuda-keyring_1.1-1_all.deb && sudo dpkg -i cuda-keyring_1.1-1_all.deb && sudo apt-get update && sudo apt-get -y install cuda-toolkit-12-5 && sudo curl -L https://ollama.com/download/ollama-linux-amd64 -o /usr/bin/ollama && sudo chmod +x /usr/bin/ollama && sudo useradd -r -s /bin/false -m -d /usr/share/ollama ollama && ollama serve
